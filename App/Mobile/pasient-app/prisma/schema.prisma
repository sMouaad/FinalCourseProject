generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URI")
}

model todos {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model users {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  v        Int    @map("__v")
  email    String @unique(map: "email_1")
  name     String
  password String
}
